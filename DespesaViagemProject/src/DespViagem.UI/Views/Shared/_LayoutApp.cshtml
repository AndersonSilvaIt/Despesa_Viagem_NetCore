<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - PW_ApontamentoManual</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/lib/font-awesome/css/all.min.css" />
    <link href="~/css/list.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link href="~/css/sideBar.css" rel="stylesheet" />
    <link href="~/toastr.js/toastr.min.css" rel="stylesheet" />

    <link href="~/css/jquery-ui.min.css" rel="stylesheet" />

    @RenderSection("Styles", required: false)
</head>
<body style="line-height: 1">

    <div class="wrapper">

        <div id="content">
            <div class="modalHeader01">
                <h2 style="color:white; padding-top: 20px;">@ViewData["Title"]</h2>
            </div>

            <div class="tab-content" style="padding: 0 20px 20px 20px;">
                <div class="container" style="padding-top: 10px;">
                    <main role="main" class="pb-3">
                        @RenderBody()
                    </main>
                </div>
            </div>
            <footer class="border-top footer text-muted">
                <div class="container">
                    &copy; @DateTime.Now.Year - Prodwin
                </div>
            </footer>
        </div>
    </div>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/lib/font-awesome/js/all.min.js"></script>
    <script src="~/toastr.js/toastr.min.js"></script>
    <script src="~/js/jquery-ui.min.js"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>
